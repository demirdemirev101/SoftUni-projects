
@{
    ViewBag.Title = $"Nums 1 to {ViewBag.Count}";
}
<h3>@ViewBag.Title</h3>
<ul>
    @for (int i = 1; i <= ViewBag.Count; i++)
    {
        <li>@i</li>
    }
</ul>
<form  asp-controller="Home" asp-action="Numbers1ToN" method="post">
    <input type="number" name="count" />

    <input type="submit" />
</form>
